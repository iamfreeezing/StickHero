<AnchorPane prefHeight="548.0" prefWidth="258.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.stickhero.gameOver">
   <children>
      <ImageView fitHeight="547.0" fitWidth="271.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@homepage.jpg" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="111.0" layoutX="32.0" layoutY="330.0" opacity="0.53" stroke="WHITE" strokeType="INSIDE" width="205.0" />
      <Button fx:id="homebutton" layoutX="109.0" layoutY="385.0" mnemonicParsing="false" onAction="#onClickHome" text="HOME" />
      <Button fx:id="playagainbutton" layoutX="93.0" layoutY="346.0" mnemonicParsing="false" onAction="#onClickPlayAgain" text="PLAY AGAIN" />
      <Text fill="WHITE" layoutX="13.0" layoutY="171.0" strokeType="OUTSIDE" strokeWidth="0.0" text="GAME OVER">
         <font>
            <Font name="Arial Black" size="36.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
